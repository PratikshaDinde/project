#include<stdio.h>
int main()
{
 int a=0;
printf("Enter the marks :");
scanf("%d",&a);

 if(a>=85 && a<=100)
{ printf("Grade is : A");
}
else if(a>=70 && a<=84)
{
printf("Grade is : B");
}
else if(a>=55 && a<=69)
{
printf("Grade is : C");
}
else if(a>= 40&& a<=54)
{
printf("Grade is : D");
}
else if (a<= 40)
{
printf("Grade is : F");
}
}
